{
  "version": 2,
  "name": "cardiology-backend",
  "buildCommand": "npm run build",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "build": {
    "env": {
      "NODE_VERSION": "22"
    }
  }
}
